int fun1(int a, int b);
short fun2(int a);
long fun3( );


int main( )
{
    int m_1=1, m_2=2, m_3=3;
    short s_1;

    m_1 = fun1(m_2, m_3);
    s_1 = fun1(&m_1);

    return 0;
}

int fun1(int a, int b)
{
    int c = a+b;
    fun3( );
    return c;
}

short fun2(int a)
{
    a = 4;
    return 4;
}

long fun3( )
{
    long s;
    return s;
}